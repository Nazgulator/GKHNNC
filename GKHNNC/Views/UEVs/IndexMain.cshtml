@model IEnumerable<GKHNNC.Models.UEV>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h3>Все записи УЭВ</h3>
    <p>
        @Html.ActionLink("Создать", "Create", null, new { @class = "btn btn-success btn-block" })
    </p>
    <table id="T" class="table table-bordered table-striped">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Adres.Adress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KodUEV)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pribor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OtEnergyRub)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OtEnergyGkal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HwEnergyRub)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HwEnergyGkal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HwVodaRub)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HwVodaM3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Adres.Adress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.KodUEV)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pribor)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OtEnergyRub)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OtEnergyGkal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HwEnergyRub)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HwEnergyGkal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HwVodaRub)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HwVodaM3)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.ActionLink("Редактировать", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Удалить", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
    <p><a href="/UEVs/Upload" class="btn btn-success btn-block">Загрузить ведомость УЭВ &raquo;</a> </p>
    <p><a href="/Home/Index" class="btn btn-success btn-block">В главное меню &raquo;</a> </p>

</body>
</html>
