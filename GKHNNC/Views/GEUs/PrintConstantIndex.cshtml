@model IEnumerable<GKHNNC.Models.PrintConstant>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout0.cshtml";
}

<h2>Основные должности</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameRP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dolgnost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <th>
            @Html.DisplayFor(modelItem => item.NameRP)
        </th>
        <td>
            @Html.DisplayFor(modelItem => item.Dolgnost)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "PCEdit", new { id = item.Id }) 
        </td>
    </tr>
}

</table>
