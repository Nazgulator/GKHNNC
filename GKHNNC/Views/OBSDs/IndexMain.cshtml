@model IEnumerable<GKHNNC.Models.OBSD>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ОБСД</title>
</head>
<body>
    <p>
        @Html.ActionLink("Создать", "Create", null, new { @class = "btn btn-success btn-block" })
    </p>
    <table id="T" class="table table-bordered table-striped">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Adres.Adress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TableService.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nachislenie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Licevoi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Adres.Adress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TableService.Type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nachislenie)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Licevoi)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.ActionLink("Редактировать", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Удалить", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
    <p><a href="/Home/Index" class="btn btn-success btn-block">В главное меню &raquo;</a> </p>
    <p><a href="/OBSDs/Upload" class="btn btn-success btn-block">Загрузить файл ОБСД &raquo;</a> </p>
</body>
</html>
